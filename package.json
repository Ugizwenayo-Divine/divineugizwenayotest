  "description": "A client is requesting you to create a system that would allow them to digitize his quizzing business where he will allow users to get questions  from the system, and they answer them. The system automatically corrects them using answer sheets in the system and gives them their performance results. They can come back to the system and view their past marks.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "admin": "cross-env NODE_ENV=development && babel-node ./src/scripts/insertAdmin.js",
    "start": "cross-env NODE_ENV=development && babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development && nodemon --exec babel-node ./src/index.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo"
  },
